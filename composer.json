{
  "name": "wizaplace/front-bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "kevinrob/guzzle-cache-middleware": "^3.2",
    "roave/security-advisories": "@dev",
    "symfony/monolog-bundle": "^3.1",
    "symfony/symfony": "^3.4.6",
    "twig/twig": "^2.4",
    "wizaplace/sdk": "dev-feat-mondialrelay@dev"
  },
  "autoload": {
    "psr-4": {
      "WizaplaceFrontBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WizaplaceFrontBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "bmancone/guzzle-stopwatch-middleware": "^1.0",
    "kphoen/sitemap-generator": "^1.4",
    "php-vcr/php-vcr": "^1.4",
    "phpstan/phpstan": "^0.10",
    "phpstan/phpstan-symfony": "^0.10.1",
    "phpunit/phpunit": "^6.5.5",
    "sensio/distribution-bundle": "^5.0",
    "squizlabs/php_codesniffer": "^3.1",
    "symfony/phpunit-bridge": "^3.3",
    "wizaplace/php-coding-standard": "^1.1"
  },
  "suggest": {
    "kphoen/sitemap-generator": "To generate sitemaps",
    "jms/i18n-routing-bundle": "To vary routes per language"
  },
  "config" : {
    "sort-packages": true
  }
}
